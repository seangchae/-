#include<cstdio>
#include<cstring>
#include<iostream>
using namespace std;
int main()
{
	//string a;
	//getline(cin, a);
	int num = 1; //
	int len;
	char a[1000000];
	scanf("%[^\n]s", a);
	len = strlen(a);
	for(int i = 0; i < len; i++)
	{
		if(a[i] == 32)
			num += 1;
	}
	if(a[0] == 32)
		num -= 1;
	if(a[len - 1] == 32)
		num -= 1;
	printf("%d", num);
	
	return 0;
}
