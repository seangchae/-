#include<cstdio>
#include<cstring>
#include<iostream>
#include<algorithm>

using namespace std;

int arr[1000000];
int main()
{
	int n = 0;
	scanf("%d", &n);
	for(int i = 0; i < n; i++)
	{
		scanf("%d", &arr[i]);
	}
	sort(arr, arr + n);
	for(int i = 0; i < n; i++)
	{
		printf("%d", arr[i]);
	}
	
	return 0;
}
